## Параметры приложения

# Ключ для доступа к OpenRouter
OPENROUTER_API_KEY=

# URL OpenRouter API
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Используемая модель
OPENROUTER_MODEL=openai/chatgpt-4o-latest

# Модель для эмбеддингов
OPENROUTER_EMBED_MODEL=text-embedding-3-small

# Опциональные параметры для рейтинга на openrouter.ai
OPENROUTER_SITE_URL=
OPENROUTER_SITE_NAME=

# Строка подключения к базе данных
DB_URL=

# Уровень логирования
LOG_LEVEL=INFO

# Язык для OCR Tesseract
TESSERACT_LANG=eng

# Папка для сохранения обработанных документов
OUTPUT_DIR=Archive
